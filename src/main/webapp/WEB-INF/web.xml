<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>controller.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainPage</servlet-name>
    <jsp-file>/WEB-INF/pages/main_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>UserPage</servlet-name>
    <jsp-file>/WEB-INF/pages/user_pages/user_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>SignInPage</servlet-name>
    <jsp-file>/WEB-INF/pages/user_pages/sign_in_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>SignUpPage</servlet-name>
    <jsp-file>/WEB-INF/pages/user_pages/sign_up_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>AlbumPage</servlet-name>
    <jsp-file>/WEB-INF/pages/obj_pages/album_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>AlbumSearchPage</servlet-name>
    <jsp-file>/WEB-INF/pages/search_pages/album_search_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>PerformerPage</servlet-name>
    <jsp-file>/WEB-INF/pages/obj_pages/performer_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>PerformerSearchPage</servlet-name>
    <jsp-file>/WEB-INF/pages/search_pages/performer_search_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>TrackPage</servlet-name>
    <jsp-file>/WEB-INF/pages/obj_pages/track_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>TrackSearchPage</servlet-name>
    <jsp-file>/WEB-INF/pages/search_pages/track_search_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>PerformerCreatePage</servlet-name>
    <jsp-file>/WEB-INF/pages/create_pages/create_performer_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>AlbumCreatePage</servlet-name>
    <jsp-file>/WEB-INF/pages/create_pages/create_album_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>TrackCreatePage</servlet-name>
    <jsp-file>/WEB-INF/pages/create_pages/create_track_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>UserSearchPage</servlet-name>
    <jsp-file>/WEB-INF/pages/search_pages/admin/user_search_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>TrackChangePage</servlet-name>
    <jsp-file>/WEB-INF/pages/change_pages/change_track_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>AlbumChangePage</servlet-name>
    <jsp-file>/WEB-INF/pages/change_pages/change_album_page.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>PerformerChangePage</servlet-name>
    <jsp-file>/WEB-INF/pages/change_pages/change_performer_page.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>TrackPage</servlet-name>
    <url-pattern>/track</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrackSearchPage</servlet-name>
    <url-pattern>/search/track</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PerformerPage</servlet-name>
    <url-pattern>/performer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PerformerSearchPage</servlet-name>
    <url-pattern>/search/performer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AlbumPage</servlet-name>
    <url-pattern>/album</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AlbumSearchPage</servlet-name>
    <url-pattern>/search/album</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignInPage</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignUpPage</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserPage</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainPage</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PerformerCreatePage</servlet-name>
    <url-pattern>/create/performer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AlbumCreatePage</servlet-name>
    <url-pattern>/create/album</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrackCreatePage</servlet-name>
    <url-pattern>/create/track</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserSearchPage</servlet-name>
    <url-pattern>/search/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrackChangePage</servlet-name>
    <url-pattern>/change/track</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AlbumChangePage</servlet-name>
    <url-pattern>/change/album</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PerformerChangePage</servlet-name>
    <url-pattern>/change/performer</url-pattern>
  </servlet-mapping>

</web-app>
